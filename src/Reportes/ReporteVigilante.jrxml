	<parameter name="CostoFinal" class="java.lang.String"/>
	<queryString>
		<![CDATA[Select p.idPersonal, CONCAT(p.nombres, ' ', p.apellidos) as nombreCompleto, tp.nombreTipo, p.idEstadoPersonal as id, p.salarioMensual FROM Personal AS p
Inner Join TipoPersonal AS tp ON p.idTipoPersonal = tp.idTipoPersonal
Inner Join EstadoPersonal AS ep ON p.idEstadoPersonal = ep.idEstadoPersonal
WHERE p.idEstadoPersonal = 1 AND p.salarioMensual >= $P{CostoInicial} AND p.salarioMensual <= $P{CostoFinal}
order by p.salarioMensual]]>
	</queryString>
	<field name="idPersonal" class="java.lang.Integer"/>
	<field name="nombreCompleto" class="java.lang.String"/>
	<field name="nombreTipo" class="java.lang.String"/>
	<field name="id" class="java.lang.Integer"/>
	<field name="salarioMensual" class="java.math.BigDecimal"/>
	<variable name="salarioMensual_1" class="java.math.BigDecimal" calculation="Lowest">
		<variableExpression><![CDATA[$F{salarioMensual}]]></variableExpression>
	</variable>
	<variable name="salarioMensual_2" class="java.math.BigDecimal" calculation="Sum">
