	<queryString>
		<![CDATA[Select h.idTurno, p.idPersonal, CONCAT(p.nombres, ' ', p.apellidos) AS nombreCompleto, h.idPorton, substring(convert(varchar, h.horaIngreso, 100), len(convert(varchar, h.horaIngreso, 100)) - 6, 7) AS horaIngreso, substring(convert(varchar, h.horaSalida, 100), len(convert(varchar, h.horaSalida, 100)) - 6, 7) AS horaSalida, FORMAT(h.fecha, 'dd-MM-yyyy') AS fecha FROM Turno AS h Inner Join Personal AS p ON h.idPersonal = p.idPersonal WHERE h.fecha >= $P{FechaInicio} AND h.fecha <= $P{FechaFin} ORDER BY p.idPersonal]]>
	</queryString>
